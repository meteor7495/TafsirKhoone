{
    "extends": [
        "next/core-web-vitals",
        "plugin:storybook/recommended",
        "plugin:@next/next/recommended",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "semi": ["error", "always"],
        "no-console": "warn",
        "no-unused-vars": "warn",
        "react/display-name": [0, { "ignoreTranspilerName": false }]
    }
}
